{% extends "layout.html.twig" %}
{% block title %}{{ parent() }} - Projects - {{ project.title }}{% endblock %}
{% block content %}
<article>
    <h2 class="title">{{ project.title }}</h2>
    <p>{{ project.description }}</p>
    <p><a href="{{ project.url }}">Visit</a></p>
</article>
{% endblock %}